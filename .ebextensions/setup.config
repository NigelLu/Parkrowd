{
    "container_commands": {
        "01_psql_devel": {
            "command": "sudo yum install postgresql-devel -y"
        },
        "02_migration": {
            "command": "python manage.py makemigrations --noinput",
            "leader_only": true
        },
        "03_migrate": {
            "command": "python manage.py migrate --noinput",
            "leader_only": true
        },
        "04_load_parking_lots": {
            "command": "python manage.py runscript load_parking_lot_data",
            "leader_only" :true
        },
        "05_collect_static": {
            "command": "python manage.py collectstatic --noinput",
            "leader_only": true
        },
        "06_change_db_file_permission": {
            "command": "sudo chmod -R 777 .",
            "leader_only": true
        }
    }
}
